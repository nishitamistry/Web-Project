<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Signup</servlet-name>
  	<display-name>Signup</display-name>
  	<description></description>
  	<servlet-class>com.signupservlet.Signup</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Signupservlet</servlet-name>
  	<display-name>Signupservlet</display-name>
  	<description></description>
  	<servlet-class>com.girfarm.signup.Signupservlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Addfarms</servlet-name>
  	<display-name>Addfarms</display-name>
  	<description></description>
  	<servlet-class>AdminServlet.Addfarms</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Signup</servlet-name>
  	<url-pattern>/Signup</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Signupservlet</servlet-name>
  	<url-pattern>/Signupservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Addfarms</servlet-name>
  	<url-pattern>/Addfarms</url-pattern>
  </servlet-mapping>
</web-app>
